{
	"root": true,
	"extends": [
		"eslint:recommended",
		"plugin:import/errors",
		"plugin:import/warnings"
	],
	"plugins": [
		"react"
	],
	"parser":"babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			jsx": true,
			"experimentalObjectRestSpread": true
		}
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"jquery": true,
		"jest": true
	},
	"rules": {
		"no-cond-assign": 1,
		"no-console": 1,
		"no-empty": 1,
		"no-extra-parens": 2,
		"no-extra-semi": 2,
		"no-irregular-whitespace": 1,
    "no-case-declarations": 0,
		"no-invalid-regexp": 2,
		"no-unreachable": 2,
		"no-unsafe-finally": 1,
		"no-unsafe-negation": 1,
		"valid-jsdoc": 1,
		"eqeqeq": 1,
		"consistent-return": 0,
		"default-case": 1,
		"no-empty-function": 2,
		"no-eval": 1,
		"no-extra-bind": 1,
		"no-invalid-this": 1,
		"react/jsx-no-undef": 1,
		"react/jsx-uses-react": 1,
		"react/jsx-uses-vars": 1,
		"react/no-danger": 1,
		"react/no-set-state": 0,
		"react/no-multi-comp": 1,
		"react/prefer-es6-class": 1,
		"react/prop-types": 1,
		"react/react-in-jsx-scope": 1,
		"import/extensions": 1
	}
}
